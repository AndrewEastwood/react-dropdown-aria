{
  "name": "react-aria-dropdown",
  "version": "1.0.0",
  "description": "Simple accessible React dropdown component",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:dev\" \"npm run demo:dev\"",
    "build": "npm run lib:prod && npm run demo:prod",
    "lib:dev": "rollup -c -w -o dist/react-aria-dropdown.dev.js",
    "lib:prod": " SET NODE_ENV=production && rollup -c -o dist/react-aria-dropdown.min.js",
    "demo:dev": "webpack-dev-server --mode development --open --hot",
    "demo:prod": "webpack --mode production",
    "deploy:demo": "npm run build && git push origin `git subtree split --prefix demo/dist master`:gh-pages --force",
    "start": "webpack-dev-server --mode development --open --hot",
    "lint": "eslint --ext .js,.jsx src/ demo/src",
    "gzip-size": "node -e \"process.stdout.write('JS gzip size: ')\" && gzip-size --raw dist/react-aria-dropdown.min.js && node -e \"process.stdout.write('CSS gzip size: ')\" && gzip-size --raw dist/react-aria-dropdown.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfangrad/react-aria-dropdown.git"
  },
  "keywords": [
    "react",
    "dropdown",
    "select",
    "aria",
    "react-dropdown",
    "react-aria-dropdown"
  ],
  "author": "Justin Fagnrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfangrad/react-aria-dropdown/issues"
  },
  "homepage": "https://github.com/jfangrad/react-aria-dropdown#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "gzip-size-cli": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.9",
    "uglifycss": "0.0.29",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
