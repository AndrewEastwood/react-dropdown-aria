{
  "name": "react-dropdown-aria",
  "version": "1.4.0",
  "description": "Simple and accessible React dropdown component",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lib:dev": "tsc -w",
    "lib:prod": "tsc",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:update": "jest --updateSnapshot"
  },
  "author": "Justin Fangrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfangrad/react-dropdown-aria/issues"
  },
  "homepage": "https://github.com/jfangrad/react-dropdown-aria#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/sinon": "^7.0.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.14",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.11",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.4",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "ts-jest": "^24.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "emotion": "^9.2.6"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(tsx?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.ts"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/index.js"
    }
  ]
}
